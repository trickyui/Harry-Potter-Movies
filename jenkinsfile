pipeline{
  agent any
  stages{
    stage('Checkout Code'){
      steps{
        git branch 'https://github.com/trickyui/Harry-Potter-Movies.git'
      }
    }

    stage('Build'){
      steps{
        sh 'echo "building application"'
      }
    }

    stage('Test'){
      steps{
        sh 'echo "Running tests"'
      }
    }

    stage('Deploy'){
      steps{
        sh 'echo "Deploying"'
      }
    }
  }
}
